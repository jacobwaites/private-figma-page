{
    "name": "Figma App Page",
    "description": "Simple express app to serve a figma url fullscreen behind a password.",
    "repository": "https://github.com/vanderlin/private-figma-page",
    "keywords": [
        "node",
        "express",
        "static",
        "figma"
    ],
    "env": {
        "COOKIE_SECRET": {
            "description": "sessions secret",
            "generator": "secret"
        },
        "SITE_PASSWORD": {
            "description": "Password for the site",
            "generator": "secret"
        },
        "SITE_TITLE": {
            "description": "Site title",
            "value": "My Figma App"
        },
        "FIGMA_URL": {
            "description": "Figma iframe url",
            "value": "https://www.figma.com/embed/xxxx"
        }
    }
}